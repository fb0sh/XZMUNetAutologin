{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "xzmunetautologin",
  "version": "1.2.0",
  "identifier": "com.xzmunetautologin.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "xzmunetautologin",
        "width": 500,
        "height": 900
        
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
  
}
